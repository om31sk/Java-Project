<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"https://www.springframework.org/dtd/spring-beans-2.0.dtd">

  <beans>
      
       <bean id="par" class="Program2.Parcel">
        
        <property name="id" value="1"/>
        <property name="name" value="DTDC"/>
        <property name="SourceAddress" value="PUNE"/>
        <property name="destAddress" value="MUMBAI"/>
        
      </bean>
      
      <bean id="c" class="Program2.Courier" autowire="byName">
      
        <property name="cname" value="ETHDC"/>
        <property name="website" value="http://ethdc.com"/>
        
      
      </bean>
      
     
      
  </beans>